

class Test{
      public static void main(String[] args){
            printNumbers(50);
      }
      
      public static void printNumbers(int i){
            if(i >= 2){
                  System.out.print(2);
            }
            for(int a = 3; a <= i ; a++){
                  int prime = 1;
                  for(int b = 2; b <= a/2; b++){
                        if(a%b == 0){
                              prime = 0;
                        }
                  }
                  if(prime == 1){
                        System.out.printf(", %d", a);
                  }
            }
      }
}