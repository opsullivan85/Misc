import java.util.*;

class KeyboardAI{
      Random rand = new Random();
      public static void main(String[] args){
            
      }
}

class Keyboard{
      public Keyboard(Random rand){
            
      }
}

class Population{
      
      Keyboard[] boards;
      int gen = 1;
      int fitnessSum;
      
      public Population(int size){
            boards = new Keyboard[size];
            for(int i = 0; i < size; i++){
                  boards[i] = new Keyboard(new Random());
            }
      }
}